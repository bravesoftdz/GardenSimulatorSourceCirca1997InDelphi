      MFD(water.autoIrrigationForDay_mm, v, ft, d);
    kSoilPatchAutoIrrigationIsByFurrow: 
      MFD(params.autoIrrigationIsByFurrow, v, ft, d);
    kSoilPatchAvgCurveNumber: 
      MFD(water.avgCurveNumber, v, ft, d);
    kSoilPatchAvgCurveNumberUnadjustedForSlope: 
      MFD(params.avgCurveNumberUnadjustedForSlope, v, ft, d);
    kSoilPatchAvgNConcInRainfall_gPm3: 
      MFD(params.avgNConcInRainfall_gPm3, v, ft, d);
    kSoilPatchBaseDrawingColor: 
      MFD(params.baseDrawingColor, v, ft, d);
