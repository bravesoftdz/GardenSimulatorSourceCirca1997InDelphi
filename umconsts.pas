procedure CreateAllAspects(aspectManager: GsAspectManager);
  begin
  if aspectManager = nil then
    raise Exception.create('CreateAllAspects: nil aspect manager');
  Bag_createAspects(aspectManager);
  Weather_createAspects(aspectManager);
  Plant_createAspects1(aspectManager);
  Plant_createAspects2(aspectManager);
  DrawingPlant_createAspects(aspectManager);
  SoilPatch_createAspects1(aspectManager);
  SoilPatch_createAspects2(aspectManager);
  SoilPatch_createAspects3(aspectManager);
  end;

