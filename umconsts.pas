procedure SoilPatch_directTransferField(soilpatchProxy: GsStreamableObject; var v;
  d, fieldID, ft, index, dM: smallint; updateList: TListCollection);
  var soilpatch: GsSoilPatch;
  begin
  if soilpatchProxy = nil then
    raise Exception.create('SoilPatch_directTransferField: nil soilpatch');
  soilpatch := soilpatchProxy as GsSoilPatch;
  with soilpatch do case fieldID of
    kSoilPatchAlbedo_frn: 
      MFD(surface.albedo_frn, v, ft, d);
    kSoilPatchAmmoniaAdsorbedToSediment_kgPha: 
      BDConvert(movement.ammoniaAdsorbedToSediment_kgPha,
          v, kSoilPatchAmmoniaAdsorbedToSediment_kgPha, dM, ft, d, index, updateList);
